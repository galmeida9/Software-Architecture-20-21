for (int i = 1; i &lt;= numberOfQuestions; i++) {
	result.append(&quot;{&quot;);
	result.append(&quot;\&quot;optionId\&quot;: &quot;);
	result.append(vars.get(&quot;option_id_&quot;+i));
	result.append(&quot;,  \&quot;timeTaken\&quot;: &quot;);
	result.append(i);
	result.append(&quot;,  \&quot;sequence\&quot;: &quot;);
	result.append(i);
	result.append(&quot;,  \&quot;questionAnswerId\&quot;: &quot;);
	result.append(vars.get(&quot;question_answer_id_&quot;+i));
	result.append(&quot;}&quot;);	
	if (i != numberOfQuestions) {
		result.append(&quot;,&quot;);
