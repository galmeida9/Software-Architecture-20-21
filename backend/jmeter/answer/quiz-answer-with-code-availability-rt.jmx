            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import java.time.ZonedDateTime
import java.time.format.DateTimeFormatter

StringBuilder result = new StringBuilder();
String newline = System.getProperty(&quot;line.separator&quot;);
int numberOfQuestions = Integer.parseInt(vars.get(&quot;number_of_questions&quot;));

for (int i = 1; i &lt;= numberOfQuestions; i++) {
	result.append(&quot;{&quot;);
	result.append(&quot;\&quot;id\&quot;: &quot;);
	result.append(vars.get(&quot;question_id_&quot;+i));
	result.append(&quot;,  \&quot;sequence\&quot;: &quot;);
	result.append(i);
	result.append(&quot;}&quot;);	
	if (i != numberOfQuestions) {
		result.append(&quot;,&quot;);
    		result.append(newline);
	}
}

vars.put(&quot;questions&quot;, result.toString());

ZonedDateTime startDate = ZonedDateTime.now();
ZonedDateTime finishDate = startDate.plusMinutes(7);
String start = startDate.format(DateTimeFormatter.ISO_OFFSET_DATE_TIME);
String end = finishDate.format(DateTimeFormatter.ISO_OFFSET_DATE_TIME);
String start2 = startDate.format(&quot;yyyy-MM-dd H:m&quot;);
String end2 = finishDate.format(&quot;yyyy-MM-dd H:m&quot;);
vars.put(&quot;startTime&quot;, start);
props.put(&quot;startTime&quot;, start2);
vars.put(&quot;endTime&quot;, end);
props.put(&quot;endTime&quot;, end2);</stringProp>
            </JSR223PreProcessor>
